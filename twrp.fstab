/boot        emmc       /dev/block/mmcblk0p15
/system      ext4       /dev/block/mmcblk0p23
/data        ext4       /dev/block/mmcblk0p26
/cache       ext4       /dev/block/mmcblk0p24
/recovery    emmc       /dev/block/mmcblk0p16
/external_sd vfat       /dev/block/mmcblk1p1   /dev/block/mmcblk1  flags=display="Micro SDcard";storage;removable
/usb-otg     vfat       /dev/block/sda1        /dev/block/sda      flags=display="USB-OTG";storage;removable
